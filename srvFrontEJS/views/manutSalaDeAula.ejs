<!DOCTYPE html>
<html lang="pt-br">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Manutenção de Salas de Aula</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <div class="container">
    <h1>Salas de Aula</h1>
    <table class="table">
      <thead>
        <tr>
          <th>ID</th>
          <th>Descrição</th>
          <th>Localização</th>
          <th>Capacidade</th>
        </tr>
      </thead>
      <tbody>
        <% salasDeAula.forEach(sala => { %>
          <tr>
            <td><%= sala.id %></td>
            <td><%= sala.descricao %></td>
            <td><%= sala.localizacao %></td>
            <td><%= sala.capacidade %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
    <a href="/" class="btn btn-secondary">Voltar</a>
  </div>
</body>
</html>